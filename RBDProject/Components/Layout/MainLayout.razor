@inherits LayoutComponentBase

<!-- Page Wrapper -->
<div id="wrapper">

    @* NAVBAR *@
    <NavMenu></NavMenu>

    <RadzenNotification></RadzenNotification>

    <!-- Content Wrapper -->
    <div id="content-wrapper" class="d-flex flex-column">

        <!-- Main Content -->
        <div id="content">

            @* TOPBAR *@
            <TopBar></TopBar>

            <!-- Begin Page Content -->
            <div class="container-fluid">
                @Body
            </div>
            <!-- /.container-fluid -->

        </div>
        <!-- End of Main Content -->

        @* FOOTER *@
        @* <Footer></Footer> *@

    </div>
    <!-- End of Content Wrapper -->

</div>
<!-- End of Page Wrapper -->
<!-- Scroll to Top Button-->
<NavLink class="scroll-to-top rounded" href="#page-top">
    <i class="fas fa-angle-up"></i>
</NavLink>



<RadzenComponents @rendermode=@InteractiveServer></RadzenComponents>

